      <dependency>
        <groupId>org.openscada.external</groupId>
        <artifactId>org.openscada.external.jcifs</artifactId>
        <version>${utgard-external.version}</version>
        <exclusions>
          <exclusion>
            <artifactId>bcprov-jdk15on</artifactId>
